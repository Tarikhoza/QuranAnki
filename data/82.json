ransliteration": "fujjirat", "word_index": 2, "sura_index": 82, "ayah": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627 \u0671\u0644\u06e1\u0628\u0650\u062d\u064e\u0627\u0631\u064f \u0641\u064f\u062c\u0651\u0650\u0631\u064e\u062a\u06e1", "ayah_translit": "wa-idha al-biharu fujjirat", "ayah_index": 2, "ayah_english": "And when the seas are made to gush forth", "word_tranlation": "are made to gush forth", "audio_file": "082_003_003.mp3"}, {"word": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627", "transliteration": "wa-idha", "word_index": 0, "sura_index": 82, "ayah": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627 \u0671\u0644\u06e1\u0642\u064f\u0628\u064f\u0648\u0631\u064f \u0628\u064f\u0639\u06e1\u062b\u0650\u0631\u064e\u062a\u06e1", "ayah_translit": "wa-idha al-quburu bu'thirat", "ayah_index": 3, "ayah_english": "And when the graves are overturned", "word_tranlation": "And when", "audio_file": "082_004_001.mp3"}, {"word": "\u0671\u0644\u06e1\u0642\u064f\u0628\u064f\u0648\u0631\u064f", "transliteration": "al-quburu", "word_index": 1, "sura_index": 82, "ayah": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627 \u0671\u0644\u06e1\u0642\u064f\u0628\u064f\u0648\u0631\u064f \u0628\u064f\u0639\u06e1\u062b\u0650\u0631\u064e\u062a\u06e1", "ayah_translit": "wa-idha al-quburu bu'thirat", "ayah_index": 3, "ayah_english": "And when the graves are overturned", "word_tranlation": "the graves", "audio_file": "082_004_002.mp3"}, {"word": "\u0628\u064f\u0639\u06e1\u062b\u0650\u0631\u064e\u062a\u06e1", "transliteration": "bu'thirat", "word_index": 2, "sura_index": 82, "ayah": "\u0648\u064e\u0625\u0650\u0630\u064e\u0627 \u0671\u0644\u06e1\u0642\u064f\u0628\u064f\u0648\u0631\u064f \u0628\u064f\u0639\u06e1\u062b\u0650\u0631\u064e\u062a\u06e1", "ayah_translit": "wa-idha al-quburu bu'thirat", "ayah_index": 3, "ayah_english": "And when the graves are overturned", "word_tranlation": "are overturned", "audio_file": "082_004_003.mp3"}, {"word": "\u0639\u064e\u0644\u0650\u0645\u064e\u062a\u06e1", "transliteration": "alimat", "word_index": 0, "sura_index": 82, "ayah": "\u0639\u064e\u0644\u0650\u0645\u064e\u062a\u06e1 \u0646\u064e\u0641\u06e1\u0633\u065e \u0645\u0651\u064e\u0627 \u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1 \u0648\u064e\u0623\u064e\u062e\u0651\u064e\u0631\u064e\u062a\u06e1", "ayah_translit": "alimat nafsun ma qaddamat wa-akharat", "ayah_index": 4, "ayah_english": "Will know a soul what it has sent forth and left behind", "word_tranlation": "Will know", "audio_file": "082_005_001.mp3"}, {"word": "\u0646\u064e\u0641\u06e1\u0633\u065e", "transliteration": "nafsun", "word_index": 1, "sura_index": 82, "ayah": "\u0639\u064e\u0644\u0650\u0645\u064e\u062a\u06e1 \u0646\u064e\u0641\u06e1\u0633\u065e \u0645\u0651\u064e\u0627 \u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1 \u0648\u064e\u0623\u064e\u062e\u0651\u064e\u0631\u064e\u062a\u06e1", "ayah_translit": "alimat nafsun ma qaddamat wa-akharat", "ayah_index": 4, "ayah_english": "Will know a soul what it has sent forth and left behind", "word_tranlation": "a soul", "audio_file": "082_005_002.mp3"}, {"word": "\u0645\u0651\u064e\u0627", "transliteration": "ma", "word_index": 2, "sura_index": 82, "ayah": "\u0639\u064e\u0644\u0650\u0645\u064e\u062a\u06e1 \u0646\u064e\u0641\u06e1\u0633\u065e \u0645\u0651\u064e\u0627 \u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1 \u0648\u064e\u0623\u064e\u062e\u0651\u064e\u0631\u064e\u062a\u06e1", "ayah_translit": "alimat nafsun ma qaddamat wa-akharat", "ayah_index": 4, "ayah_english": "Will know a soul what it has sent forth and left behind", "word_tranlation": "what", "audio_file": "082_005_003.mp3"}, {"word": "\u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1", "transliteration": "qaddamat", "word_index": 3, "sura_index": 82, "ayah": "\u0639\u064e\u0644\u0650\u0645\u064e\u062a\u06e1 \u0646\u064e\u0641\u06e1\u0633\u065e \u0645\u0651\u064e\u0627 \u0642\u064e\u062f\u0651\u064e\u0645\u064e\u062a\u06e1 \u0648\u064e\u0623\u064e\u062e\u0651\u064e\u0631\u064e\u062a\u0